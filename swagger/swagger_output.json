{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "API TCC",
    "description": "Ambiente com todos os endpoints referentes a API do TCC para Tecnologias Móveis e para a Internet ",
    "contact": {
      "name": "Renner Borges",
      "email": "rennerferreira23@gmail"
    }
  },
  "servers": [
    {
      "url": "http://localhost:7780",
      "description": "Local"
    }
  ],
  "tags": [
    {
      "name": "Autenticação",
      "description": "Responsável por todo gerenciamento da autenticação no software"
    },
    {
      "name": "Usuários",
      "description": "Responsável pelo gerenciamento dos usuários"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "tags": [
          "Home"
        ],
        "description": "Rota de teste da API",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "security": []
      }
    },
    "/login": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "description": "Rota responsável por autenticar um usuário",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Não autorizado"
          }
        },
        "security": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            }
          }
        }
      }
    },
    "/users": {
      "get": {
        "tags": [
          "Usuários"
        ],
        "description": "Rota responsável por trazer todos usuários",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/user/{email}": {
      "get": {
        "tags": [
          "Usuários"
        ],
        "description": "Rota responsável por trazer um usuário específico pelo e-mail do mesmo",
        "parameters": [
          {
            "name": "email",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Email do usuário"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Não Encontrado"
          }
        }
      }
    },
    "/user": {
      "post": {
        "tags": [
          "Usuários"
        ],
        "description": "Rota responsável por criar um usuário",
        "parameters": [],
        "responses": {
          "201": {
            "description": "Criado"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostUser"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SendEmailDefault": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "rennerferreira23@gmail.com"
          },
          "name": {
            "type": "string",
            "example": "Renner"
          }
        }
      },
      "ConfirmPresence": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "rennerferreira23@gmail.com"
          }
        }
      },
      "PostUser": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Renner Borges"
          },
          "password": {
            "type": "string",
            "example": "12345678@Re"
          },
          "email": {
            "type": "string",
            "example": "rennerferreira23@gmail.com"
          },
          "role": {
            "type": "string",
            "example": "g"
          },
          "enabled": {
            "type": "boolean",
            "example": true
          }
        }
      },
      "EditUser": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Renner Ferreira"
          }
        }
      },
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "renner@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "12345678@Re"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "apiKey",
        "name": "token",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
}